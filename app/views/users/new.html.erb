<%= form_with scope: :user, url: users_path, local: true do |f| %>
    <p>
        <p>
            <%= f.label :email %></br>
            <%= f.text_field :email %>
        </p>

        <p>
            <%= f.label :password %></br>
            <%= f.text_field :password %>
        </p>

        <p>
            <%= f.submit %>
        </p>

    </p>
<% end %>
