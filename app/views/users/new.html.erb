<header class="header">
    <div class="container">
        <div class="navbar">
                <%= link_to 'Home', root_path, {:class => "a"} %>
            <nav>
            <ul>
                <li class="link">
                    <a href="#">
                        About
                    </a>
                </li>
                <li class="link">
                    <%= link_to 'Sign up', new_user_path %>
                </li>
            </ul>
            </nav>
        </div>
    </div>
</header>



<%= form_with scope: :user, url: users_path, local: true do |f| %>
    <p>
        <p>
            <%= f.label :email %></br>
            <%= f.text_field :email %>
        </p>

        <p>
            <%= f.label :password %></br>
            <%= f.text_field :password %>
        </p>

        <p>
            <%= f.submit %>
        </p>

    </p>
<% end %>
